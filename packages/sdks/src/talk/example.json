{
  "children": [
    {
      "name": "div",
      "bindings": {
        "css": {
          "code": "{\n  display: 'flex',\n  flexDirection: 'columns'\n}"
        },
        "onClick": {
          "code": "trackClick(props.content.id)"
        }
      },
      "children": [
        {
          "name": "RenderBlocks",
          "bindings": {
            "blocks": {
              "code": "props.content.blocks"
            }
          }
        }
      ]
    }
  ],
  "hooks": {
    "onMount": {
      "code": "sendComponentsToVisualEditor(props.customComponents)"
    },
    "onUpdate": [
      {
        "code": "dispatchNewContentToVisualEditor(props.content);",
        "deps": ["content"]
      }
    ]
  },
  "context": {
    "set": {
      "name": "BuilderContext",
      "value": {
        "content": {
          "type": "getter",
          "code": "get content() { return props.content; }"
        },
        "registeredComponents": {
          "type": "getter",
          "code": "get registeredComponents() { return props.customComponents; }"
        }
      }
    }
  },
  "name": "RenderContent"
}
